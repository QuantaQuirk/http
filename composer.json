{
    "name": "quantaquirk/http",
    "description": "The QuantaQuirk Http package.",
        "license": "proprietary",
    "authors": [
        {
            "name": "Tomáš Selič (Smisch-DEV)",
            "email": "contact@smisch.sk"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-filter": "*",
        "fruitcake/php-cors": "^1.2",
        "guzzlehttp/uri-template": "^1.0",
        "quantaquirk/collections": "^10.0",
        "quantaquirk/macroable": "^10.0",
        "quantaquirk/session": "^10.0",
        "quantaquirk/support": "^10.0",
        "symfony/http-foundation": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/mime": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "QuantaQuirk\\Http\\": ""
        }
    },
    "suggest": {
        "ext-gd": "Required to use QuantaQuirk\\Http\\Testing\\FileFactory::image().",
        "guzzlehttp/guzzle": "Required to use the HTTP Client (^7.5)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "10.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
